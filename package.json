{
  "name": "@areyesdev/components",
  "version": "0.0.0-development",
  "private": false,
  "description": "A components library to create apps from zero to production",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/areyesdev/components-app-productivity.git"
  },
  "scripts": {
    "dev": "yarn storybook",
    "test": "jest",
    "lint": "yarn lint-js && yarn lint-css",
    "lint-js": "eslint .",
    "lint-css": "stylelint \"**/*.css\"",
    "storybook": "start-storybook -p 6006 -s ./assets",
    "build-tokens": "node ./scripts/build-tokens",
    "build-storybook": "build-storybook -s ./assets",
    "create-component": "node ./scripts/create-component"
  },
  "keywords": [],
  "author": "Andres Reyes <areyesdev@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "7.14.5",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/addon-storyshots": "6.3.4",
    "@storybook/react": "^6.3.4",
    "babel-jest": "27.0.6",
    "babel-loader": "^8.2.2",
    "chromatic": "5.10.0-canary.0",
    "enquirer": "2.3.6",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.4.2",
    "lint-staged": ">=10",
    "minireset.css": "0.0.7",
    "prettier": "^2.3.2",
    "react-test-renderer": "17.0.2",
    "semantic-release": "^17.4.4",
    "stylelint": "13.7.2",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-prettier": "1.1.2"
  },
  "peerDependencies": {
    "minireset.css": "0.0.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "build-tokens && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
