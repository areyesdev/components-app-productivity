{
  "name": "@areyesdev/components",
  "version": "0.3.0",
  "private": false,
  "description": "A components library to create apps from zero to production",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/areyesdev/components-app-productivity.git"
  },
  "engines": {
    "node": ">=14.17.3",
    "yarn": ">=1.22.10"
  },
  "scripts": {
    "build:storybook": "build-storybook",
    "build:tokens": "node ./scripts/build-tokens",
    "deploy:vercel": "vercel --prod",
    "deploy": "run-s build:* deploy:*",
    "dev:storybook": "start-storybook -p 6006",
    "dev:tokens": "watch 'yarn build:tokens' ./tokens",
    "dev": "concurrently 'npm:dev:*'",
    "lint:css:prettier": "prettier '**/*.css' --write",
    "lint:css:stylelint": "stylelint '**/*.css' --fix",
    "lint:css": "yarn lint:css:stylelint && yarn lint:css:prettier",
    "lint:js": "eslint --cache --fix",
    "lint": "run-p lint:*",
    "cc": "node ./scripts/create-component",
    "test": "jest"
  },
  "keywords": [],
  "author": "Andres Reyes <areyesdev@gmail.com>",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "7.14.5",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/addon-storyshots": "6.3.4",
    "@storybook/react": "^6.3.4",
    "babel-jest": "27.0.6",
    "babel-loader": "^8.2.2",
    "chromatic": "5.10.0-canary.0",
    "concurrently": "5.3.0",
    "classnames": "^2.3.1",
    "enquirer": "2.3.6",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.4.2",
    "lint-staged": ">=10",
    "minireset.css": "0.0.7",
    "npm-run-all": "4.1.5",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "17.0.2",
    "semantic-release": "^17.4.4",
    "stylelint": "13.7.2",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-recommended": "3.0.0",
    "watch": "1.0.2"
  },
  "peerDependencies": {
    "classnames": "^2.3.1",
    "minireset.css": "0.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "yarn lint:js",
    "*.css": "yarn lint:css"
  }
}
